{
    "name": "mabrouk/project-setting",
    "description": "a laravel package to handle dealing with project setting in groups",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Ahmed Mabrouk",
            "email": "a.mabrouk25@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": ">=9",
        "mabrouk/translatable": ">=1.0",
        "mabrouk/filterable": ">=1.0",
        "mabrouk/mediable": ">=3"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0@dev"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mabrouk\\ProjectSetting\\ProjectSettingServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Mabrouk\\ProjectSetting\\": "src/",
            "Database\\Factories\\": "src/database/factories/",
            "Database\\Seeders\\": "src/database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    }
}
